#include <bits/stdc++.h>

using namespace std;


 // } Driver Code Ends
//User function template for C++
class Solution{
public:
	string removeDuplicates(string s) {
	    string ss="";
	   int a[256]={0};
        for(int i=0;i<s.length();i++){
            if(a[s[i]]==0){
                ss+=s[i];
                a[s[i]]++;
            }
        }
            return ss;
        }
};

// { Driver Code Starts.

int main() {
    int t;
    cin >> t;
    while (t--) {
        string str;
        cin >> str;
        Solution ob;
        auto ans = ob.removeDuplicates(str);

        cout << ans << "\n";
    }
    return 0;
}  
